<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="300dp"
    android:layout_height="match_parent"
    android:background="@color/black">

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp">

            <TextView
                android:id="@+id/maintenancelog_tzdbh_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="通知单编号："
                android:textSize="9dp"
                android:textColor="@color/white"/>

            <EditText
                android:id="@+id/maintenancelog_tzdbh_ev"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:background="@drawable/line"
                android:textSize="9dp"
                android:imeOptions="flagNoExtractUi"
                android:textColor="@color/white"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp">

            <TextView
                android:id="@+id/maintenancelog_wxdw_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="维修单位："
                android:textSize="9dp"
                android:textColor="@color/white"/>

            <EditText
                android:id="@+id/maintenancelog_wxdw_ev"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:background="@drawable/line"
                android:textSize="9dp"
                android:imeOptions="flagNoExtractUi"
                android:textColor="@color/white"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp">

            <TextView
                android:id="@+id/maintenancelog_qfrq_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="签发日期："
                android:textSize="9dp"
                android:textColor="@color/white"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <EditText
                    android:id="@+id/maintenancelog_qfrq_ev"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="@drawable/line"
                    android:textSize="9dp"
                    android:imeOptions="flagNoExtractUi"
                    android:textColor="@color/white"/>
                <TextView
                    android:layout_alignParentRight="true"
                    android:id="@+id/maintenancelog_qfrq_clean"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:textColor="@color/white"
                    android:text="清除"/>
            </RelativeLayout>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/maintenancelog_qfrqend_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:visibility="gone">

            <TextView
                android:id="@+id/maintenancelog_qfrqend_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="签发日期："
                android:textSize="9dp"
                android:textColor="@color/white"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <EditText
                    android:id="@+id/maintenancelog_qfrqend_ev"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="@drawable/line"
                    android:textSize="9dp"
                    android:imeOptions="flagNoExtractUi"
                    android:textColor="@color/white"/>
                <TextView
                    android:layout_alignParentRight="true"
                    android:id="@+id/maintenancelog_qfrqend_clean"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:visibility="gone"
                    android:text="清除"/>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp">

            <TextView
                android:id="@+id/maintenancelog_qfr_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="签发人："
                android:textSize="9dp"
                android:textColor="@color/white"/>

            <EditText
                android:id="@+id/maintenancelog_qfr_ev"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:background="@drawable/line"
                android:textSize="9dp"
                android:imeOptions="flagNoExtractUi"
                android:textColor="@color/white"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>