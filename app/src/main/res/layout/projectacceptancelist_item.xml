<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#90808080" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="#80ffffff"
        >
        <TextView
            android:id="@+id/projectacceptancelist_item_tv1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="20160613001"
            android:textSize="@dimen/list_textSice"
            android:textColor="@color/yuninfo_black_shadow"
            android:gravity="center"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#90808080" />
        <TextView
            android:id="@+id/projectacceptancelist_item_tv2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="G20"
            android:textSize="@dimen/list_textSice"
            android:textColor="@color/yuninfo_black_shadow"
            android:gravity="center"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#90808080" />
        <TextView
            android:id="@+id/projectacceptancelist_item_tv3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="青银高速公路"
            android:textSize="@dimen/list_textSice"
            android:textColor="@color/yuninfo_black_shadow"
            android:gravity="center"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#90808080" />

        <TextView
            android:id="@+id/projectacceptancelist_item_tv4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="8:30-11:00"
            android:textSize="@dimen/list_textSice"
            android:textColor="@color/yuninfo_black_shadow"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"/>
        <View
            android:id="@+id/projectacceptancelist_item_line5"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#90808080" />

        <TextView
            android:id="@+id/projectacceptancelist_item_tv5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="上传"
            android:textColor="@color/yuninfo_black_shadow"
            android:background="#ff9900"
            android:padding="4dp"
            android:textSize="@dimen/btn_upLoadSice"
            android:paddingTop="8dp"
            android:paddingBottom="8dp"/>
      

    </LinearLayout>

</LinearLayout>